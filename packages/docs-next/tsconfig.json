{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "baseUrl": ".",
    "types": [ "node", "vite/client"],
  },
  "include": [
    ".vitepress/**/*",
    "docgen.config.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}